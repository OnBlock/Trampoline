{
  "schemaVersion": 1,
  "id": "trampoline",
  "version": "${version}",

  "name": "Trampoline",
  "description": "Adds support for Bungeecord IP-Forwarding on fabric servers",
  "authors": [
    "i509VCB",
    "Forked by CODY_AI"
  ],
  "contact": {
    "homepage": "https://onblock.xyz/",
    "sources": "https://github.com/OnBlock/Trampoline?organization=OnBlock&organization=OnBlock"
  },

  "license": "MIT",

  "environment": "server",
  "entrypoints": {
    "server": [
      "xyz.onblock.fabric.trampoline.Trampoline"
    ]
  },
  "mixins": [
    "trampoline.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.4.0"
  },
  "suggests": {
  },
  "custom": {
    "fabric-networking:shouldHandshake": false
  }
}
